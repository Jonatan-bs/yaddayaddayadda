.followbox
  if userData.image
      img( alt='' style=`background-image:url(https://res.cloudinary.com/kuko/w_80/${profile.image})`)
  else
      img(style="background-image:url('/../images/profile.jpg')" alt='')
  h2
    a(href=`/profile/${userData._id}`) 
      = `${userData.firstname} ${userData.lastname}`
  h3 
    = `@${userData.username}`
  .followingfollowers
    .following
      h4 Following
      h5 
       = `${userData.following.length}`
    .followers
      h4 Followers
      h5 XX